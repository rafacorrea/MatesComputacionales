digraph Automata {
	node [shape=circle]
		"['q0']"
		"['q1']" [peripheries=2]
		"['q0', 'q1']" [peripheries=2]
			"['q0', 'q1']" -> "['q0', 'q1']" [label=1]
			"['q0', 'q1']" -> "['q0', 'q1']" [label=0]
			"['q1']" -> "['q0', 'q1']" [label=1]
			"['q1']" -> "['\\other']" [label=0]
			"['q0']" -> "['q1']" [label=1]
			"['q0']" -> "['q0', 'q1']" [label=0]
		A
		C [label="" fixedsize=true height=0]
			C -> A
}